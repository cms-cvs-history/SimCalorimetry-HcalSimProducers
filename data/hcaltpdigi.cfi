module hcalTriggerPrimitiveDigis = HcalTrigPrimDigiProducer {
// E/Gamma path scale in MeV (needed to seed the transcoder)
  int32 egammaScaleMeV = 500
// Jet path scale in MeV (needed to seed the transcoder)
  int32 jetScaleMeV = 1000
// Input digi label (_must_ be without zero-suppression!)
  string inputLabel = 'hcalDigis'
}
